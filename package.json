{
  "name": "nestjs-oauth-typeorm-react-starter",
  "version": "0.2.2",
  "description": "Nestjs/oAuth/TypeORM/Reactjs Starter",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run backend:dev\" \"npm run frontend:dev\"",
    "build": "npm run backend:build && npm run frontend:build",
    "backend:dev": "nest start --watch",
    "backend:start": "nest start",
    "backend:build": "nest build",
    "frontend:dev": "webpack-dev-server --inline --hot --progress --content-base public",
    "frontend:build": "webpack",
    "frontend:analyze": "source-map-explorer 'build/*.js'",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "test": "jest"
  },
  "author": "jyutzio@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jyutzio/nestjs-oauth-typeorm-react-starter.git"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/typeorm": "^7.1.4",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "ioredis": "^4.18.0",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-google-oauth": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "sqlite3": "^5.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.4",
    "typeorm": "^0.2.28",
    "typescript": "^4.0.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/testing": "^7.4.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/connect-redis": "0.0.14",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.48",
    "@types/ioredis": "^4.17.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/passport": "^1.0.4",
    "@types/passport-github2": "^1.2.4",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.4.2",
    "jest-css-modules": "^2.1.0",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.0",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
