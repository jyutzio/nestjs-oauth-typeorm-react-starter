{
  "name": "nestjs-oauth-typeorm-react-starter",
  "version": "0.2.1",
  "description": "Nestjs/oAuth/TypeORM/Reactjs Starter",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run backend:dev\" \"npm run frontend:dev\"",
    "build": "npm run backend:build && npm run frontend:build",
    "backend:dev": "nest start --watch",
    "backend:start": "nest start",
    "backend:build": "nest build",
    "frontend:dev": "webpack-dev-server --inline --hot --progress --content-base public",
    "frontend:build": "webpack",
    "frontend:analyze": "source-map-explorer 'build/*.js'",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "test": "jest"
  },
  "author": "jyutzio@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jyutzio/nestjs-oauth-typeorm-react-starter.git"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/config": "0.4.0",
    "@nestjs/core": "^7.0.7",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.7",
    "@nestjs/typeorm": "^7.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.17.0",
    "helmet": "^3.22.0",
    "ioredis": "^4.16.1",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-google-oauth": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "sqlite3": "^4.1.1",
    "style-loader": "^1.1.3",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/testing": "^7.0.7",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@types/connect-redis": "0.0.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.4",
    "@types/express-rate-limit": "^5.0.0",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.45",
    "@types/ioredis": "^4.14.9",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/passport": "^1.0.3",
    "@types/passport-github2": "^1.2.4",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-testing-library": "^3.0.2",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.2.7",
    "jest-css-modules": "^2.1.0",
    "prettier": "^2.0.2",
    "source-map-explorer": "^2.4.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
